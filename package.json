{
  "name": "tempest",
  "version": "0.7.1",
  "description": "League of Legends companion app",
  "homepage": "./",
  "keywords": [
    "league of legends",
    "lol",
    "lcu"
  ],
  "author": "",
  "license": "ISC",
  "main": "./build-main/app/electron.js",
  "build": {
    "appId": "nonExistentID",
    "files": [
      "build-main/**/*",
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extends": null
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^3.1.5",
    "apollo-boost": "^0.4.9",
    "apollo-link-error": "^1.1.13",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.2",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "lcu-connector": "^2.1.3",
    "node-fetch": "^2.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-toastify": "^6.0.6",
    "three": "^0.118.3",
    "typescript": "^3.9.3",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/electron": "^1.6.10",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.12.47",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/ws": "^7.2.5",
    "concurrently": "^5.2.0",
    "electron": "9.0.5",
    "electron-builder": "^22.6.1",
    "electron-react-devtools": "^0.5.3",
    "electron-reload": "^1.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.1.6",
    "wait-on": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-web": "react-app-rewired start",
    "build": "react-scripts build",
    "build-web": "react-app-rewired build",
    "build-main": "tsc --project ./app/tsconfig.json",
    "build-main-prod": "npm run build-main -- --transpileOnly",
    "typecheck-main": "npm run build-main -- --noEmit --watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-electron": "electron .",
    "preelectron-pack": "concurrently \"npm run build\" \"npm run build-main\"",
    "watch-ts-electron": "cross-env NODE_ENV=dev npm run build-main && npm run start-electron",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && npm run watch-ts-electron\"",
    "electron-pack": "electron-builder build",
    "eb": "electron-builder"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
